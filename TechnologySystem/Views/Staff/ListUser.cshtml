@model IEnumerable<TechnologySystem.Models.ApplicationUser>
@using TechnologySystem.ViewModels
@{
    ViewBag.Title = "ListUser";
}
<h2>ListUser</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">No.</th>
            <th scope="col">User Name</th>
            <th scope="col">Full name</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @{int i = 0;}
        @foreach (var user in Model)
        {
            <tr>
                <th scope="row">@{i++;}@i</th>
                <td>@user.UserName</td>
                <td>@user.FullName</td>
            </tr>
        }
    </tbody>
</table>
