@model TechnologySystem.Models.ApplicationUser
@{
    ViewBag.Title = "Index";
}

<h2>My Information</h2>

<div class="panel panel-info">
    <div class="panel-body">
        <p>Name: @Model.FullName</p>
        <p>Age: @Model.Age</p>
        <p>Date of Birth: @Model.DateofBirth</p>
        <p>Address: @Model.Address</p>
        <p>Education: @Model.Education</p>
    </div>
</div>



